 syn: CREATE TABLE [IF NOT EXISTS] table_name(  
      column_definition1,  
      column_definition2,  
      ........,  
      table_constraints  
);  
 
 CREATE TABLE employee_table(  
    id int NOT NULL AUTO_INCREMENT,  
    name varchar(45) NOT NULL,  
    occupation varchar(35) NOT NULL,  
    age int NOT NULL,  
    PRIMARY KEY (id)  
);